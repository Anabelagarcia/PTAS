<h1>Conquistas Cadastradas</h1>

<a href="/usuarios/{{usuario.id}}/novaConquista">Cadastrar conquista</a>

<table border="1">
  <!-- table row -->
  <tr>
    <!-- table head -->
    <th>Título</th>
    <th>Descrição</th>
  </tr>
  {{#each Conquistas}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.descricao}}</td>
    </tr>
  {{/each}}
</table>